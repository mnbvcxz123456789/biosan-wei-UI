input,textarea{box-sizing:border-box}progress{border-radius:10rpx}.bs-select{position:relative}.bs-select:first-child::before{content:" ";position:absolute;left:30rpx;top:0;right:30rpx;height:1px;border-top:1rpx solid #d8d8d8}.bs-select-row{padding-left:10px;position:relative;display:flex}.bs-select-row::after{content:" ";position:absolute;left:30rpx;bottom:0;right:30rpx;height:1px;border-bottom:1rpx solid #d8d8d8}.bs-select-text{display:flex;align-items:center;flex:1;padding-left:10px}.bs-select-icon{color:#1aad19}